package org.example;

public class HoursWork {
    private String Hour;
    private String date;

    public HoursWork(String hour, String date) {
        Hour = hour;
        this.date = date;
    }

    public String getHour() {
        return Hour;
    }

    public void setHour(String hour) {
        Hour = hour;
    }

    public String getDate() {
        return date;
    }

    public void setDate(String date) {
        this.date = date;
    }
}
